{
  "name": "commonps",
  "displayName": "Common Pseudo Code Syntax Highlighter",
  "description": "Common Pseudo Code",
  "version": "0.1.0",
  "publisher": "adarshrkumar",
  "icon": "logo_rounded.png",
  "scripts": {
    "test": "echo \"Test Succesfull!\"",
    "publish": "vsce package && vsce publish minor",
    "next-version": "node nextVersion.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/adarshrkumar/pscode.git"
  },
  "bugs": {
    "url": "https://github.com/adarshrkumar/pscode/issues"
  },
  "homepage": "https://github.com/adarshrkumar/pscode#readme",
  "engines": {
    "vscode": "^1.105.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "commonps",
        "aliases": [
          "Common Pseudo Code",
          "commonps"
        ],
        "extensions": [
          ".pscode",
          ".pcode",
          ".pseudo",
          ".commonps"
        ],
        "configuration": "./language-configuration.json",
        "icon": {
          "light": "./icon.svg",
          "dark": "./icon.svg"
        }
      }
    ],
    "grammars": [
      {
        "language": "commonps",
        "scopeName": "source.commonps",
        "path": "./syntaxes/commonps.tmLanguage.json"
      }
    ],
    "iconThemes": [
      {
        "id": "commonps-icons",
        "label": "Common Pseudo Code Icons",
        "path": "./fileicons.json"
      }
    ]
  }
}
